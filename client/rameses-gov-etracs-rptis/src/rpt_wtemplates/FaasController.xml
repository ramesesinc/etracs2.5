<?xml version="1.0" encoding="UTF-8"?>
<workunit>
    <invokers>
        <invoker type="navActions" action="previewTaxDec" caption="View Tax Declaration" immediate="true" visibleWhen="#{mode=='read'}"/>
        <invoker type="navActions" action="viewAnnotations" caption="Annotations" immediate="true" visibleWhen="#{mode=='read' &amp;&amp; faas.annotated == true}"/>
        <invoker type="navActions" action="previewFaas" caption="View FAAS" immediate="true" visibleWhen="#{mode=='read'}"/>
    </invokers>

    <pageflow>
        <start>
            <transition name="dcinit" to="dcinit"/>
            <transition name="txninit" to="txninit"/>
            <transition name="ndinit" to="ndinit"/>
            <transition name="mcinit" to="mcinit"/>
            <transition name="grinit" to="grinit"/>
            <transition name="open" to="open"/>
        </start>
        
        <process name="open">
            <transition to="default" cond="#{faas.state == 'INTERIM'}"/>
            <transition to="forapproval" cond="#{faas.state == 'FORAPPROVAL'}"/>
            <transition to="forprovsubmission" cond="#{faas.state == 'FORPROVSUBMISSION'}"/>
            <transition to="forprovapproval" cond="#{faas.state == 'FORPROVAPPROVAL'}"/>
            <transition to="current" cond="#{faas.state == 'CURRENT'}"/>
            <transition to="cancelled" cond="#{faas.state == 'CANCELLED'}"/>
            <transition to="readonly" cond="#{faas.state == 'PENDING'}"/>
        </process>
        
        <page name="dcinit" title="Data Capture : Initial">
            <transition caption="Close" to="end"  mnemonic="c" icon="images/toolbars/cancel.png"/>
            <transition caption="Next"  to="newpage" action="initDataCaptureFaas" mnemonic="n"  />
        </page>
        
        <page name="txninit" title="FAAS Transaction : Initial">
            <transition caption="Close" to="end"  mnemonic="c" icon="images/toolbars/cancel.png"/>
            <transition caption="Next"  to="newpage" action="initFaasTransaction" mnemonic="n"  />
        </page>
        
        <page name="ndinit" title="New Discovery : Initial">
            <transition caption="Close" to="end"  mnemonic="c" icon="images/toolbars/cancel.png"/>
            <transition caption="Next"  to="newpage" action="initNewDiscoveryFaas" mnemonic="n"  />
        </page>
        
        <page name="mcinit" title="Multiple Claim: Initial">
            <transition caption="Close" to="end"  mnemonic="c" icon="images/toolbars/cancel.png"/>
            <transition caption="Next"  to="newpage" action="initMultipleClaimFaas" mnemonic="n"  />
        </page>
        
        <page name="grinit" title="General Revision: Initial">
            <transition caption="Close" to="end"  mnemonic="c" icon="images/toolbars/cancel.png"/>
            <transition caption="Next"  to="newpage" action="initGeneralRevisionFaas" mnemonic="n"  />
        </page>
        
        <page name="newpage" title="FAAS #{faas.txntype.name} (Interim)">
            <transition caption="Cancel" to="end"      mnemonic="c" icon="images/toolbars/cancel.png" confirm="Cancel entry?"/>
            <transition caption="Save"   to="back-to-default" action="save" name="save" mnemonic="s" shortcut="ctrl S" icon="images/toolbars/save.png"  visibleWhen="#{mode != 'read'}" />
        </page>
        
        <page name="default" title="FAAS #{faas.txntype.name} (Interim)">
            <transition caption="Close"  to="end"      mnemonic="c" icon="images/toolbars/cancel.png" visibleWhen="#{mode == 'read'}"/>
            <transition caption="Edit"   to="back-to-default" action="edit"  name="edit" mnemonic="e" shortcut="ctrl E" icon="images/toolbars/edit.png"  visibleWhen="#{mode == 'read'}" />
            <transition caption="Cancel" to="back-to-default" action="cancelEdit" name="cancel" mnemonic="c" icon="images/toolbars/cancel.png" visibleWhen="#{mode != 'read'}"/>
            <transition caption="Save"   to="back-to-default" action="save" name="save" mnemonic="s" shortcut="ctrl S" icon="images/toolbars/save.png"  visibleWhen="#{mode != 'read'}" />
            <transition caption="Delete" to="delete"  action="delete" confirm="Delete record?"    mnemonic="d" icon="images/toolbars/trash.png"  visibleWhen="#{mode == 'read'}" />
            <transition caption="Submit" to="submit" action="submitFaasForApproval"  mnemonic="m"  visibleWhen="#{mode == 'read'}" confirm="Submit FAAS for approval?"/>
        </page>
        
        <process name="back-to-default">
            <transition to="default" />
        </process>
        
        <process name="delete">
            <transition to="end"/>
        </process>
        
        <process name="submit">
            <transition to="forapproval" cond="#{faas.state == 'FORAPPROVAL'}"/>
            <transition to="forprovapproval" cond="#{faas.state == 'FORPROVAPPROVAL'}"/>
            <transition to="forprovsubmission" cond="#{faas.state == 'FORPROVSUBMISSION'}"/>
        </process>
        
        
        <!-- CITY-->
        <page name="forapproval" title="FAAS #{faas.txntype.name} (For Approval)">
            <transition caption="Close" to="end" mnemonic="c" icon="images/toolbars/cancel.png" visibleWhen="#{mode == 'read'}" />
            <transition caption="Edit"   to="back-to-forapproval" action="edit" name="edit" mnemonic="e" shortcut="ctrl E" icon="images/toolbars/edit.png"  visibleWhen="#{mode == 'read'}" />
            <transition caption="Cancel" to="back-to-forapproval" action="cancelEdit" name="cancel"  mnemonic="c" icon="images/toolbars/cancel.png" visibleWhen="#{mode != 'read'}"/>
            <transition caption="Save"   to="back-to-forapproval" action="save" name="save" mnemonic="s" shortcut="ctrl S" icon="images/toolbars/save.png"  visibleWhen="#{mode != 'read'}" />
            <transition caption="Disapprove" to="default" action="disapproveFaas" mnemonic="d" visibleWhen="#{mode == 'read'}" confirm="Disapprove FAAS?"/>
            <transition caption="Approve" to="current" action="approveFaas" mnemonic="v" icon="images/toolbars/approve.png" visibleWhen="#{mode == 'read'}" confirm="Approve FAAS?"/>
        </page>
        
        <process name="back-to-forapproval">
            <transition to="forapproval"/>
        </process>
        
        
        <!-- MUNICIPALIY -->
        <page name="forprovsubmission" title="FAAS #{faas.txntype.name} (For Submission to Province)">
            <transition caption="Close"  to="end" mnemonic="c" icon="images/toolbars/cancel.png" visibleWhen="#{mode == 'read'}" />
            <transition caption="Edit"   to="back-to-prov-submission" action="edit" name="edit" mnemonic="e" shortcut="ctrl E" icon="images/toolbars/edit.png"  visibleWhen="#{mode == 'read'}" />
            <transition caption="Cancel" to="back-to-prov-submission" action="cancelEdit" name="cancel"  mnemonic="c" icon="images/toolbars/cancel.png" visibleWhen="#{mode != 'read'}"/>
            <transition caption="Save"   to="back-to-prov-submission" action="save" name="save" mnemonic="s" shortcut="ctrl S" icon="images/toolbars/save.png"  visibleWhen="#{mode != 'read'}" />
            <transition caption="Disapprove" to="default" action="disapproveFaasSubmissionToProvince"  mnemonic="d" visibleWhen="#{mode == 'read'}" confirm="Disapprove FAAS?"/>
            <transition caption="Submit to Province" to="provsubmissionapproval" action="submitFaasToProvince" mnemonic="m" visibleWhen="#{mode == 'read'}" confirm="Submit FAAS to province?"/>
        </page>
        
        <process name="back-to-prov-submission">
            <transition to="forprovsubmission"/>
        </process>
        
        <page name="provsubmissionapproval" title="FAAS #{faas.txntype.name} (For Province Approval)">
            <transition caption="Close" to="end" mnemonic="c" icon="images/toolbars/cancel.png"/>
            <transition caption="Disapprove" to="forprovsubmission" action="disapproveFaasByProvince"  mnemonic="d" confirm="Disapprove FAAS?"/>
            <transition caption="Approve" to="current" action="approveFaasByProvince"  mnemonic="v" confirm="Submit FAAS for approval?"/>
        </page>
        
        
        <!-- PROVINCE -->
        <page name="forprovapproval" title="FAAS #{faas.txntype.name} (Province Approval)">
            <transition caption="Close" to="end" mnemonic="c" icon="images/toolbars/cancel.png" visibleWhen="#{mode == 'read'}" />
            <transition caption="Edit"   to="back-to-forprovapproval" action="edit" name="edit" mnemonic="e" shortcut="ctrl E" icon="images/toolbars/edit.png"  visibleWhen="#{mode == 'read'}" />
            <transition caption="Cancel" to="back-to-forprovapproval" action="cancelEdit" name="cancel"  mnemonic="c" icon="images/toolbars/cancel.png" visibleWhen="#{mode != 'read'}"/>
            <transition caption="Save"   to="back-to-forprovapproval" action="save" name="save" mnemonic="s" shortcut="ctrl S" icon="images/toolbars/save.png"  visibleWhen="#{mode != 'read'}" />
            <transition caption="Disapprove" to="default" action="disapproveFaasByProvince"  mnemonic="d"  visibleWhen="#{mode == 'read'}" confirm="Disapprove FAAS?"/>
            <transition caption="Approve" to="current" action="approveFaasByProvince"  mnemonic="v" visibleWhen="#{mode == 'read'}" confirm="Approve FAAS?" />
        </page>
        
        <process name="back-to-forprovapproval">
            <transition to="forprovapproval"/>
        </process>
        
        <page name="current" title="FAAS #{faas.txntype.name} (Current)">
            <transition caption="Close" to="end" mnemonic="c" icon="images/toolbars/cancel.png"/>
        </page>
        
        <page name="cancelled" title="FAAS #{faas.txntype.name} (Cancelled)">
            <transition caption="Close" to="end" mnemonic="c" icon="images/toolbars/cancel.png"/>
        </page>
        
        <page name="readonly" title="FAAS #{faas.txntype.name} (View Only)">
            <transition caption="Close" to="end" mnemonic="c" icon="images/toolbars/cancel.png"/>
        </page>
        
        
        <end />
        
    </pageflow>
    
    <pages> 
        <page name="dcinit" template="com.rameses.gov.etracs.rpt.faas.ui.FAASDataCaptureInitPage"/>
        <page name="txninit" template="com.rameses.gov.etracs.rpt.faas.ui.FAASTxnInitPage"/>
        <page name="newpage" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="default" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="forapproval" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="forprovsubmission" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="provsubmissionapproval" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="forprovapproval" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="current" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="cancelled" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="readonly" template="com.rameses.gov.etracs.rpt.faas.ui.FAASPage"/>
        <page name="preview" template="com.rameses.gov.etracs.rpt.report.common.PreviewPage" />
    </pages>
</workunit>


