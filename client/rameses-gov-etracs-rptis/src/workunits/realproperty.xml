<?xml version="1.0" encoding="UTF-8"?>
<workunit>
    <invokers>
        <invoker type="quick_launcher" code="RP01" caption="Real Property" role="APPRAISER,TAXMAPPER" target="popup" />
        <invoker type="realproperty:create" action="init" caption="Real Property" role="APPRAISER,TAXMAPPER" target="popup"/>
        <invoker type="realproperty:open" action="open" caption="Real Property" role="APPRAISER,TAXMAPPER"  target="popup"/>
        
        <invoker type="extActions" action="close" caption="Close" mnemonic="c" visibleWhen="#{mode == 'read'}" />
        <invoker type="extActions" action="cancelCreate" caption="Cancel" mnemonic="c" visibleWhen="#{mode == 'create'}" immediate="true" />
        <invoker type="extActions" action="init" caption="New" mnemonic="n" shortcut="ctrl N" visibleWhen="#{mode == 'read' &amp;&amp; allowCreate == true}" role="APPRAISER,TAXMAPPER" xpermission="realproperty.create"/>
        <invoker type="extActions" action="create" caption="Save" mnemonic="s" shortcut="ctrl S" visibleWhen="#{mode == 'create'}"  />
        <invoker type="extActions" action="edit" caption="Edit" mnemonic="e" shortcut="ctrl E" visibleWhen="#{showEditAction == true}" role="APPRAISER,TAXMAPPER" xpermission="realproperty.edit"/>
        <invoker type="extActions" action="cancelEdit" caption="Cancel" mnemonic="c" visibleWhen="#{mode == 'edit'}" immediate="true" />
        <invoker type="extActions" action="update" caption="Save" mnemonic="s" shortcut="ctrl S" visibleWhen="#{mode == 'edit'}" />
        <invoker type="extActions" action="delete" caption="Delete" mnemonic="d" visibleWhen="#{showDeleteAction == true}" role="APPRAISER,TAXMAPPER" xpermission="realproperty.delete"/>
        <invoker type="extActions" action="approve" caption="Approve" mnemonic="a" visibleWhen="#{showApproveAction == true}" role="APPRAISER,TAXMAPPER" xpermission="realproperty.delete"/>
    </invokers>
        
    <code class="com.rameses.gov.etracs.rpt.realproperty.ui.RealPropertyController">
    </code>
    
    <pages>
        <page template="com.rameses.gov.etracs.rpt.realproperty.ui.RealPropertyPage" />
    </pages>
</workunit>
