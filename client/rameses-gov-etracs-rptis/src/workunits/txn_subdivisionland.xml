<workunit>
    <invokers>
        <invoker type="subdividedland:create" action="create" caption="Subdivided Land (New)" mnemonic="n" shortcut="ctrl N" target="popup" icon="images/doc-new.png"/>
        <invoker type="subdividedland:open" action="open" caption="Subdivided Land (Edit)" mnemonic="o" shortcut="ctrl O" target="popup" icon="images/edit.png"/>
        
        <invoker type="formActions" action="_close" caption="Close" immediate="true" icon="images/toolbars/cancel.png" />
        <invoker type="formActions" action="validateRealProperty" caption="Next" icon="images/toolbars/next.png" visibleWhen="#{step=='rp'}"/>
        
        <invoker type="formActions" action="backToRealPropertyPage" caption="Back" icon="images/toolbars/back.png" visibleWhen="#{step=='rpu'}" immediate="true" />
        <invoker type="formActions" action="validateRpu" caption="Next" icon="images/toolbars/next.png" visibleWhen="#{step=='rpu'}"/>
        
        <invoker type="formActions" action="backToRpuPage" caption="Back" icon="images/toolbars/back.png" visibleWhen="#{step=='info'}" immediate="true" />
        <invoker type="formActions" action="addLand" caption="Add" mnemonic="a" visibleWhen="#{onadd != null &amp;&amp; step=='info'}" icon="images/doc-add.png"/>
        <invoker type="formActions" action="updateLand" caption="Update" mnemonic="u" visibleWhen="#{onupdate != null &amp;&amp; step=='info' }" icon="images/save.png"/>
    </invokers>
    
    <code class="com.rameses.gov.etracs.rpt.subdivision.ui.SubdividedLandController" >
    </code>
    
    <pages>
        <page name="rp" template="com.rameses.gov.etracs.rpt.realproperty.ui.RealPropertyPage" />
        <page name="rpu" template="com.rameses.gov.etracs.rpt.subdivision.ui.SubdivisionLandRpuPage" />
        <page name="info" template="com.rameses.gov.etracs.rpt.subdivision.ui.SubdividedLandInfoPage" />
    </pages>
</workunit>

