"entity.*" [allowEdit == true]{
    enabled:true;
}


"entity.*" [allowEdit == false]{
    enabled:false;
}


"entity.pintype|entity.barangay|entity.isection|entity.iparcel|entity.claimno" [entity.datacapture == true || allowEditPinInfo == false]{
    enabled:false;
}

"entity.isection|entity.iparcel" [allowEditSectionParcel == true]{
    enabled:true;
}

