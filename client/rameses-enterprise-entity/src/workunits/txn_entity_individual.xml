<?xml version="1.0" encoding="UTF-8"?>
<workunit extends="entity_wtemplates/entity.xml">
    <invokers>
        <invoker folderid="/explorer/txn/entity" caption="Individual" action="init" target="window" xrole="ENTITY_ENCODER" xpermission="individual.create" index="1"/>  
        <invoker type="entityindividual:open" action="open" caption="Individual" target="popup" xrole="ENTITY_ENCODER" xpermission="individual.create"/>
        <invoker type="entityindividual:create" action="init" caption="Individual" target="popup" xrole="ENTITY_ENCODER" xpermission="individual.create"/>
        
        <invoker type="formActions" action="enrollOnline" visibleWhen="#{mode == 'view' and entity.objid != null and entity.useraccount==null}" immediate="true" caption="Enroll Online" mnemonic="o" xrole="ENTITY_ENCODER" xpermission="individual.enrollonline"/>
        <invoker type="formActions" action="editAccount" visibleWhen="#{mode == 'view' and entity.objid != null and entity.useraccount!=null}" immediate="true" caption="Edit Online Account" xrole="ENTITY_ENCODER" xpermission="individual.enrollonline"/>
    </invokers>
    <code class="com.rameses.enterprise.entity.controllers.EntityIndividualTxnController" />
    <pages>
        <page template="com.rameses.enterprise.entity.ui.EntityIndividualPage"/>
    </pages>
</workunit>