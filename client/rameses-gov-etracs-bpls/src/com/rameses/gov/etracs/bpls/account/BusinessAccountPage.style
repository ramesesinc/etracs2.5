"entity.*"[ mode != 'read' ] {
    enabled: true;
}

"entity.*"[  mode == 'read' ] {
    enabled: false;
}

"entity.objid"[  mode != 'create' ] {
    enabled: false;
}

"entity.(surcharge|interest|department)"[  entity.taxfeetype == 'TAXCREDIT' ] {
    visible: false;
}

"entity.(surcharge|interest|department)"[  entity.taxfeetype != 'TAXCREDIT' ] {
    visible: true;
}

