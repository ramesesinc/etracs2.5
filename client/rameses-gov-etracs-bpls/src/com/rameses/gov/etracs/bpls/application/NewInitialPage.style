"entity.(dtfiled|yearstarted|appyear)" [ entity.txnmode != "CAPTURE" ] {
    visible:false;
}
"entity.(dtfiled|yearstarted|appyear)" [ entity.txnmode == "CAPTURE" ] {
    visible:true;
}

"entity.branchname" [ entity.officetype == "BRANCH" ] {
    visible:true;
}
"entity.branchname" [ entity.officetype != "BRANCH" ] {
