        <field name="basicintpartial" required="true" type="decimal" />
        <field name="sef" required="true" type="decimal" />
        <field name="sefint" required="true" type="decimal" />
        <field name="sefdisc" required="true" type="decimal" />
        <field name="sefcredit" required="true" type="decimal" />
        <field name="sefpaid" required="true" type="decimal" />
        <field name="sefintpartial" required="true" type="decimal" />

        <field name="basicacctid"/>
        <field name="basicintacctid" />
        <field name="sefacctid"/>
