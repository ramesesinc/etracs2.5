import com.rameses.annotations.*;
import com.rameses.common.*;

class RPTParameterListService
{
	@PersistenceContext('main')
	def em

	@Service('ListUtilService')
	def listSvc 
	
	@ProxyMethod
	public List getList( Map params ) {
		return listSvc.getList( 'rptparameter:getList', params )
	}

	@ProxyMethod
    public List getFieldList() {
        return [
            [objid:'SYS_BASE_MARKET_VALUE', name:'SYS_BASE_MARKET_VALUE', caption:'SYSTEM BASE MARKET VALUE'],
            [objid:'SYS_MARKET_VALUE', name:'SYS_MARKET_VALUE', caption:'SYSTEM MARKET VALUE'],
            [objid:'SYS_BASE_VALUE', name:'SYS_BASE_VALUE', caption:'SYSTEM BASE VALUE'],
            [objid:'SYS_UNIT_VALUE', name:'SYS_UNIT_VALUE', caption:'SYSTEM UNIT VALUE'],
            [objid:'SYS_AREA', name:'SYS_AREA', caption:'SYSTEM AREA'],
            [objid:'SYS_ASSESSED_VALUE', name:'SYS_ASSESSED_VALUE', caption:'SYSTEM ASSESSED VALUE'],
        ]
    }

    @ProxyMethod
    public List getVariableList() {
        return getFieldList() + getList(null)
    }
	
}
