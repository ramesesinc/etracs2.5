import com.rameses.annotations.*;
import com.rameses.eserver.*;
import com.rameses.common.*;

class MachineLookupService
{
	@PersistenceContext('main')
	def em

	@Service('LookupUtilService')
	def utilSvc 
	
	@ProxyMethod
	public def lookupMachines( params ) {
		return utilSvc.lookup( 'machine:lookupMachines', params )
	}
}
