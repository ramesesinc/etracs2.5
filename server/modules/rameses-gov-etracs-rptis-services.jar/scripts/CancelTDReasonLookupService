import com.rameses.annotations.*;
import com.rameses.eserver.*;
import com.rameses.common.*;
import java.rmi.server.UID;

class CancelTDReasonLookupService
{
	@PersistenceContext('main')
	def em

	@Service('LookupListService')
	def lookupSvc 
	
	@ProxyMethod
	public def lookupCancelTDReasons( params ) {
		return lookupSvc.lookup( 'canceltdreason:lookupCancelTDReasons', params )
	}
	
}
