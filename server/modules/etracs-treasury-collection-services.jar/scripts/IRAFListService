import com.rameses.common.*import com.rameses.annotations.*class IRAFListService{	@PersistenceContext('main')	def em		@ProxyMethod	public List getList( params ) {		params.searchtext = (params.searchtext? params.searchtext+'%':'%');		params.riv_txnno = params.searchtext;		params.riv_requestedby_name = params.searchtext;				def qry = em.sqlContext.createNamedQuery('iraf:getList');		if( params ) {			qry.setParameters(params);			if( params._start ) qry.setFirstResult(params._start);			if( params._limit ) qry.setMaxResults(params._limit);		}		return qry.resultList;	}}