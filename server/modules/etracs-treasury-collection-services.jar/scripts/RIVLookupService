import com.rameses.common.*;import com.rameses.annotations.*;class RIVLookupService{	@PersistenceContext('main')	def em;		@ProxyMethod	public List getList( params ) {		params.searchtext = (params.searchtext? params.searchtext+'%':'%');		params.txnno = params.searchtext;		params.requestedby_name = params.searchtext;				def qry = em.sqlContext.createNamedQuery('riv:getListByOpenState');		if( params ) {			qry.setParameters(params);			if( params._start ) qry.setFirstResult(params._start);			if( params._limit ) qry.setMaxResults(params._limit);		}		return qry.resultList;	}}