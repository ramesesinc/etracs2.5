<schema>
	<element tablename="afissue" >
		<field name="objid" primary="true"/>
		<field name="state" required="true"/>
		<field name="active" type="integer" required="true"/>
		<field name="mode" required="true"/>
		<field name="afinventorycreditid" />
		<field name="dtissued" required="true"/>
		<field name="txndate" />
		<field name="collector_objid" required="true"/>
		<field name="collector_name" required="true"/>
		<field name="collector_title" required="true"/>
		<field name="stubno" required="true"/>
		<field name="af_objid" required="true"/>
		<field name="af_type" required="true"/>
		<field name="assignedto_objid" />
		<field name="assignedto_name" />
		<field name="assignedto_title" />
		<field name="prefix"/>
		<field name="suffix"/>
		<field name="startseries" type="integer" required="true"/>
		<field name="endseries" type="integer" required="true"/>
		<field name="currentseries" type="integer" required="true"/>
		<field name="beginqty" type="integer" />
		<field name="receivedqty" type="integer" required="true"/>
		<field name="issuedqty" type="integer" />
		<field name="endqty" type="integer" />
		<complex name="cancelledseries" ref="cancelledseries" type="list"/>
	</element>
	
	<element name="cancelledseries">
		<field name="series" type="integer" required="true"/>
		<field name="remarks" required="true"/>
	</element>
	
	<element name="transfer" tablename="afcontroltransfer">
		<field name="objid" primary="true"/>
		<!-- APPROVED -->
		<field name="docstate" required="true" />
		<field name="txndate" required="true" />
		<field name="afcontrolid" required="true" />
		<field name="transferafcontrolid" required="true" />
		<field name="collectorid" required="true" />
		<field name="transfercollectorid" required="true" />
		
	</element>
	
</schema>
