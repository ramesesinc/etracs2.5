declare LOBFact
	classificationid:	String
	objid:				String
end

declare LOBAttributeFact
	lob:				LOBFact
	classificationid:	String
	objid:				String
end

rule "rule1"
	when
		lob: LOBFact : ( classificationid == "L0120102012" )
	then
		BusinessInfo bf = new BusinessInfo();
		bf.setLob( lob );
		bf.setId( "BV00001" );
		bf.setName(  "NUM_EMPLOYEES" );
		items.add( bf );
end