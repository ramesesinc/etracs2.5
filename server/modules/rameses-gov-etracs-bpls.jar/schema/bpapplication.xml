<schema>
	<element tablename="bpapplication">
		<field name="objid" primary="true"/>
		<field name="state" required="true"/>
		<field name="appno" required="true"/>
		<field name="apptype" required="true"/>
		<field name="dtfiled" required="true"/>
		<field name="refapplicationid" />
		<field name="permitee_name" required="true"/>
		<field name="permitee_objid" required="true"/>
		<field name="barangay_objid" required="true"/>
		<field name="barangay_name" required="true"/>
		<field name="tradename" required="true"/>
		<field name="businessaddress" required="true"/>
		<field name="orgtype" required="true"/>
		<field name="officetype" required="true"/>
		<complex name="infos" required="true" type="list" serializer="default"/>
		<field name="iyear" required="true"/>
	</element>	

	<element name="lob" tablename="bpapplication_lob">
		<field name="objid" primary="true"/>
		<field name="parentid" required="true"/>
		<field name="lobid" required="true"/>
		<field name="name" required="true"/>
		<field name="assessmenttype" required="true"/>
	</element>	

	<element name="info" tablename="bpapplication_info">
		<field name="objid" primary="true"/>
		<field name="parentid" required="true"/>
		<field name="attribute_objid" required="true"/>
		<field name="attribute_name" required="true"/>
		<field name="lob_objid" />
		<field name="lob_name" />
		<field name="decimalvalue" />
		<field name="intvalue" />
		<field name="stringvalue" />
		<field name="boolvalue" />
		<complex name="listvalue" type="list" serializer="default"/>
	</element>	

	<element name="taxfee" tablename="bpapplication_taxfee">
		<field name="objid" primary="true"/>
		<field name="parentid" required="true"/>
		<field name="account_objid" required="true"/>
		<field name="account_code" required="true"/>
		<field name="account_title" required="true"/>
		<field name="lob_objid" />
		<field name="lob_name" />
		<field name="org_objid"/>
		<field name="org_name" />
		<field name="assessedvalue" required="true"/>
		<field name="amount" required="true"/>
		<field name="remarks"/>
		<field name="recurring" required="true"/>
	</element>	

	<element name="requirement" tablename="bpapplication_requirement">
		<field name="objid" primary="true"/>
		<field name="parentid" required="true"/>
		<field name="reftype" required="true"/>
		<field name="refid" required="true"/>
		<field name="refno" required="true"/>
		<field name="title" required="true"/>
		<field name="remarks" required="true"/>
		<field name="status" required="true"/>
	</element>	

</schema>