import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;

public class BusinessListService {
	
	@ProxyMethod
	public def getNodes( def p ) {
		def list = [
			[name:'open', caption:'Open'  ],
			[name:'engineering', caption:'Engineering'],
			[name:'forapproval', caption:'For Approval'],
		];
	}

	@ProxyMethod
	public def getColumns( def p ) {
		return [
			[name:'tradename', caption:'Business Name'],
			[name:'permitee.name', caption:'Permitee']
		]	
	}

	@ProxyMethod
	public def getList( def p ) {
		return [];
	}

}