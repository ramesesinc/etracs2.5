import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.services.extended.*

public class InactiveBusinessService extends ActiveCrudListService {

	@ActiveDB("inactivebusiness")
	def em;

	String pagingKeys = "a.objid";

	public def getColumns(def o) {
		return [
			[name:"tradename", caption:"Tradename"],
			[name:"businessaddress", caption:"Bus.Address"],
			[name:"owner.name", caption:"Owner"],
			[name:"owner.address", caption:"Owner Address"],
		];
	}


}
