		<field name="issuedby_name" required="true"/>
		<field name="expirydate" required="true"/>
		<field name="remarks" />
	</element>	

	<element name="changeinfo" tablename="business_changeinfo">
		<field name="objid" primary="true"/>
		<field name="businessid" required="true"/>
		<field name="state" required="true"/>
		<field name="reftype" required="true"/>
		<field name="refno" required="true"/>
		<field name="refdate" required="true"/>
		<field name="oldvalue" required="true"/>
		<field name="newvalue" required="true"/>
		<field name="issuedby_objid" required="true"/>
		<field name="issuedby_name" required="true"/>
	</element>

	<element name="redflag" tablename="business_redflag">
		<field name="objid" primary="true"/>
		<field name="businessid" required="true"/>
		<field name="message" required="true"/>
		<field name="flagid" required="false"/>
	</element>	


</schema>