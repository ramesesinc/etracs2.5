		<field name="intvalue" />
		<field name="stringvalue" />
		<field name="boolvalue" />
		<field name="phase" />
		<field name="level" />
	</element>	

	<element name="case" tablename="business_case">
		<field name="objid" primary="true"/>
		<field name="businessid" required="true"/>
		<field name="state" required="true"/>
		<field name="casetype" required="true"/>
		<field name="description" required="true"/>
	</element>	

	<element name="permit" tablename="business_permit">
		<field name="objid" primary="true"/>
		<field name="businessid" required="true"/>
		<field name="applicationid" required="true"/>
		<field name="activeyear" required="true"/>
		<field name="version" required="true"/>
		<field name="permitno" required="true"/>
