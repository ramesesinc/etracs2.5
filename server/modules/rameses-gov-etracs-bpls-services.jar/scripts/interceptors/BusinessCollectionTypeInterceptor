import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.services.extended.*;

public class BusinessCollectionTypeInterceptor {

	@ActiveDB("businessaccount")
	def em;

	
	@After(pattern="CollectionTypeService.open", eval="result.handler == 'bpls'")
	public def getAccounts(def evt) { 
		def ct = evt.result; 
		ct.items = em.getList();
	}


}