import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.services.extended.*;

public class BusinessCaptureService  {

	@ActiveDB(value="businesscapture", em="tracs")
	def em;

	@ProxyMethod
	public def getSearchList(o) { 
		o.tradename = o.tradename ? o.tradename+'%' : '%' 
		o.ownername = o.ownername ? o.ownername+'%' : '%' 
		o._pagingKeys = 'b.ObjID'
		o._limit  = 25
		return em.getSearchList(o);
	}

	@ProxyMethod
	public def open(o) {
		o = em.findBusinessInfo( o )
		//[em.findLob]
		o.infos = [];
		
		o.infos << [ lob:[name:'SARI SARI'], attribute:[caption:'CAPITAL'], value: 15000.0 ];
		return o;


	}

}