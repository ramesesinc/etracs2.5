import com.rameses.annotations.*
import com.rameses.common.*;

class XAccountLookupService
{
	@PersistenceContext('rpt')
	def em 

	@ProxyMethod
	public def getList( params ) {
		return [
			[objid:'BASIC CURRENT', title:'RPT BASIC CURRENT'],
			[objid:'BASIC INTEREST', title:'RPT BASIC INTEREST'],
			[objid:'SEF CURRENT', title:'SEF CURRENT'],
			[objid:'SEF INTEREST', title:'RPT SEF INTEREST'],
		]
	}

	@ProxyMethod
	public def getColumns( params ) {
		return [ 
			[name:'title', caption:'Account Title'],
		]	
	}
}

